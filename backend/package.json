    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.18.0",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.0"
